				"params": {
					"title": "Copy"
				}
			},
			{
				"type": "button",
				"key": "d",
				"fromModule": {
					"callback": "deleteFrame"
				},
				"params": {
					"title": "Delete Frame"
				}
			},
			{
				"type": "button",
				"key": "shift-d",
				"fromModule": {
					"callback": "deleteFrameRange"
				},
				"params": {
					"title": "Delete Frame Range"
				}
			},
			{
				"type": "button",
				"key": "g",
				"fromModule": {
					"callback": "duplicate"
				},
				"params": {
					"title": "Duplicate"
				}
			},
			{
				"type": "button",
				"key": "i",
				"fromModule": {
					"callback": "insertFrameBefore"
				},
				"params": {
					"title": "Insert Before"
				}
			},
			{
				"type": "button",
				"key": "shift-i",
				"fromModule": {
					"callback": "insertFrameAfter"
				},
				"params": {
					"title": "Insert After"
				}
			},
			{
				"type": "button",
				"key": "m",
				"fromModule": {
					"callback": "addMultipleCopies"
				},
				"params": {
					"title": "Multi Copies"
				}
