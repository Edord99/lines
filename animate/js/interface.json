{
	"mouse": {
		"id": "mouse-menu",
		"label": "Mouse",
		"module": "drawEvents",
		"list": [
			{
				"type": "range",
				"key": "u",
				"fromModule": {
					"value": "mouseInterval",
					"callback": "setMouseInterval"
				},
				"params": {
					"min": 0,
					"max": 100,
					"input": "mouse-range",
					"label": "Mouse time"
				}
			}
		]
	},
	"background-image": {
		"id": "background-image-menu",
		"label": "BG image",
		"module": "background",
		"list": [
			{
				"type": "text",
				"fromModule": {
					"callback": "loadImage"
				},
				"params": {
					"title": "Image URL, Enter",
					"id": "bg-img"
				}
			},
			{
				"type": "toggle",
				"key": "b",
				"fromModule": {
					"callback": "toggle"
				},
				"params": {
					"on": "Hide",
					"off": "Show"
				}
			},
			{
				"type": "range",
				"fromModule": {
					"callback": "setX"
				},
				"params": {
					"label": "X",
					"min": -1024,
					"max": 1024,
					"value": 0,
					"input": "x-range"
				}
			},
			{
				"type": "range",
				"fromModule": {
					"callback": "setY"
				},
				"params": {
					"label": "Y",
					"min": -1024,
					"max": 1024,
					"value": 0,
					"input": "y-range"
				}
			},
			{
				"type": "range",
				"fromModule": {
					"callback": "setSize"
				},
				"params": {
					"label": "Size",
					"input": "size-range",
					"min": 0.1,
					"max": 2,
					"value": 1,
					"step": 0.1
				}
			}
		]
	},
	"canvas": {
		"id": "canvas-menu_",
		"label": "Canvas",
		"module": "canvas",
		"list": [
			{
				"type": "button",
				"key": "shift-f",
				"params": {
					"title": "Fit Canvas"
				},
				"fromModule": {
					"callback": "fitCanvasToDrawing"
				}
			},
			{
				"type": "text",
				"fromModule": {
					"placeholder": "width",
					"callback": "setWidth"
				},
				"params": {
					"id": "canvas-width",
					"blur": true,
					"label": "Width"
				}
			},
			{
				"type": "text",
				"fromModule": {
					"placeholder": "height",
					"callback": "setHeight"
				},
				"params": {
					"id": "canvas-height",
					"blur": true,
					"label": "Height"
				}
			}
		]	
	},
	"line-color": {
		"id": "line-color-menu",
		"label": "Line Color",
		"module": "lineColor",
		"list": [
			{
				"type": "color",
				"fromModule": {
					"callback": "set",
					"value": "color"
				}
			},
			{
				"type": "ui",
				"params": {
					"id": "line-color-ways",
					"label": "Colors"
				}
			}
		]
	},
	"bg-color": {
		"id": "bg-color-menu",
		"label": "BG Color",
		"module": "bgColor",
		"list": [
			{
				"type": "color",
				"fromModule": {
					"callback": "set",
					"value": "color"
				}
			},
			{
				"type": "ui",
				"params": {
					"id": "bg-color-ways",
					"label": "Colors"
				}
			}
		]
	},
	"data": {
		"id": "data-menu",
		"label": "Edit",
		"module": "data",
		"list": [
			{
				"type": "button",
				"key": "c",
