			{
				"type": "button",
				"key": "ctrl-z",
				"fromModule": {
					"callback": "undo"
				},
				"params": {
					"title": "Undo"
				}
			},
			{
				"type": "button",
				"key": "ctrl-v",
				"fromModule": {
					"callback": "clearFramesToCopy"
				},
				"params": {
					"title": "Clear Frames to Copy"
				}
			},
			{
				"type": "button",
				"key": "v",
				"fromModule": {
					"callback": "pasteFrames"
				},
				"params": {
					"title": "Paste Frames"
				}
			},
			{
				"type": "button",
				"key": "shift-v",
				"fromModule": {
					"callback": "selectAll"
				},
				"params": {
					"title": "Select All"
				}
			},
			{
				"type": "button",
				"key": "shift-v",
				"fromModule": {
					"callback": "selectAll"
				},
				"params": {
					"title": "Select All"
				}
			},
			{
				"type": "button",
				"key": "alt-v",
				"fromModule": {
					"callback": "selectRange"
				},
				"params": {
					"title": "Select Range"
				}
			}
		]
	},
	"explode": {
		"id": "explode-menu",
		"label": "Explode",
		"module": "data",
		"list": [
			{
				"type": "button",
				"key": "a",
				"fromModule": {
					"callback": "explode"
				},
				"params": {
					"title": "Explode",
					"arguments": {
						"follow": false,
						"over": false
					}
				}
			},
			{
				"type": "button",
				"key": "shift-a",
				"fromModule": {
					"callback": "explode"
				},
				"params": {
					"title": "Explode Over",
					"arguments": {
						"follow": false,
						"over": true
					}
				}
			},
			{
				"type": "button",
				"key": "ctrl-a",
				"fromModule": {
					"callback": "explode"
				},
				"params": {
					"title": "Follow",
					"arguments": {
						"follow": true,
						"over": false
					}
				}
			},
			{
				"type": "button",
				"key": "alt-a",
				"fromModule": {
					"callback": "explode"
				},
				"params": {
					"title": "Follow Over",
					"arguments": {
						"follow": true,
						"over": true
					}
				}
			},
			{
				"type": "button",
				"key": "shift-r",
				"fromModule": {
					"callback": "reverse"
				},
				"params": {
					"title": "Reverse Draw",
					"arguments": {
						"simultaneous": false
					}
				}
			},
			{
				"type": "button",
				"key": "alt-r",
				"fromModule": {
					"callback": "reverse"
				},
				"params": {
					"title": "Reverse Multi",
					"arguments": {
						"simultaneous": true
					}
				}
			}
		]
	}
}