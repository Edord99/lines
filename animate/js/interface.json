			{
				"type": "button",
				"key": "q",
				"fromModule": {
					"callback": "offsetDrawing"
				},
				"params": {
					"title": "Offset"
				}
			},
			{
				"type": "button",
				"key": "shift-q",
				"fromModule": {
					"callback": "offsetAll"
